{
  "name": "vue-modaltor",
  "description": "Modal plugin for Nuxt.js and Vue.js",
  "version": "1.5.3",
  "author": "davodasalnifakor@gmail.com <davodaslanifakor@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/davodaslanifakor/modaltor"
  },
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-library": "vue-cli-service build --target lib --name vue-modaltor ./src/install.js",
    "lint": "vue-cli-service lint"
  },
  "main": "./dist/modaltor.common.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "node-sass": "7.0.0",
    "sass-loader": "8.0.0",
    "@vue/compiler-sfc": "^3.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "files": [
    "dist/*",
    "src/components/*"
  ]
}
